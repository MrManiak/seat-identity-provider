{
    "name": "mrmaniak/seat-identity-provider",
    "description": "Turn SeAT into an enterprise identity provider for 3rd party services.",
    "type": "seat-plugin",
    "minimum-stability": "dev",
    "version": "1.0.0",
    "prefer-stable": true,
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "PaidProgramming",
            "email": "no.reply@localhost.local"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mrmaniak\\Seat\\IdentityProvider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrmaniak\\Seat\\IdentityProvider\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "eveseat/services": "^5.0",
        "eveseat/eveapi": "^5.0",
        "eveseat/web": "^5.0",
        "eveseat/api": "^5.0",
        "robrichards/xmlseclibs": "^3.1",
        "steverhoades/oauth2-openid-connect-server": "^3.0",
        "nyholm/psr7": "^1.8",
        "symfony/psr-http-message-bridge": "^6.0|^7.0",
        "league/oauth2-server": "^9.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrmaniak\\Seat\\IdentityProvider\\SeatIdentityProviderServiceProvider"
            ]
        }
    },
    "config": {
        "platform": {
            "ext-posix": "0",
            "ext-redis": "0",
            "ext-pcntl": "0"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
